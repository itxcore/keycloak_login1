# Development Environment Configuration
VITE_APP_NAME=My Vue App
VITE_APP_VERSION=1.0.0

# Keycloak Configuration for Development
VITE_KEYCLOAK_URL=https://auth.utribe.app
VITE_KEYCLOAK_REALM=utribe
VITE_KEYCLOAK_CLIENT_ID=giftportal

# API Configuration
VITE_API_BASE_URL=http://localhost:8000
VITE_API_TIMEOUT=10000

# Security Configuration
VITE_ENABLE_AUTH_DEBUG=true
VITE_TOKEN_REFRESH_BUFFER_TIME=300000

# Feature Flags
VITE_ENABLE_GOOGLE_LOGIN=true
VITE_ENABLE_REMEMBER_ME=true
VITE_ENABLE_AUTO_LOGIN=false

# Development Settings
VITE_LOG_LEVEL=debug
VITE_ENABLE_DEVTOOLS=true